{
    "name": "tasks",
    "displayName": "Tasks",
    "description": "Load VSCode Tasks into Status Bar.",
    "version": "0.3.7",
    "publisher": "actboy168",
    "repository": {
        "type": "git",
        "url": "https://github.com/actboy168/vscode-tasks"
    },
    "bugs": {
        "url": "https://github.com/actboy168/vscode-tasks/issues"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "task",
        "statusbar"
    ],
    "icon": "logo.png",
    "engines": {
        "vscode": "^1.37.0"
    },
    "activationEvents": [
        "workspaceContains:.vscode/tasks.json"
    ],
    "main": "./extension.js",
    "contributes": {
        "jsonValidation": [{
            "fileMatch": "tasks.json",
            "url": "./jshintrc.tasks.json"
        }],
        "configuration": {
            "properties": {
                "tasks.default.statusbar.hide": {
                    "type": "boolean",
                    "default": false
                }
            }
        }
    }
}
